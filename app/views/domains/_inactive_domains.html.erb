<% unless @inactive_domains.empty? %>

<div class="card">
	<div class="card-content">
		<span class="card-title">
		 Inactive Domains</span>
     <ul>
       <% @inactive_domains.each do |domain| %>
         <% if can? :read, domain %>
           <li><%= link_to domain.name, domain_path(domain) %></li>
         <% end %>
       <% end %>
     </ul>
     </div> </div>
		 <% end %>
