<div class="card">
	<div class="card-content">
		<span class="card-title">
		 Domain List</span>


<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th class="centered">Options</th>
    </tr>
  </thead>
  <tbody>
    <% @domains.each do |domain| %>
      <% if can? :read, domain %>
        <tr>
          <td><%= link_to domain.name, domain_path(domain) %></td>
          <td>
            <%= link_to(edit_domain_path(domain), class: 'button tiny radius') do %>
						<i class="material-icons" title="Edit Domain">edit</i>&nbsp;&nbsp;

            <% end %>
          </td>
        </tr>
      <% end %>
    <% end %>
  </tbody>
</table>
<%= will_paginate @domains, previous_label: "<i class='material-icons'>navigate_before</i>", next_label: "<i class='material-icons'>navigate_next</i>" %>


<p>&nbsp;</p>
<%= render :partial => "partials/add_new_object", locals: {model_name: 'domain'} %>

</div> </div>
