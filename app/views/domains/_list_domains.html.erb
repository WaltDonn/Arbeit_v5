<div class="card">
	<div class="card-content">
		<span class="card-title">
		 Domain List</span>


<table class="table table-striped">
  <thead>
    <tr>
      <th>Name</th>
      <th class="centered">Options</th>
    </tr>
  </thead>
  <tbody>
    <% @domains.each do |domain| %>
      <% if can? :read, domain %>
        <tr>
          <td><%= link_to domain.name, domain_path(domain) %></td>
          <td>
            <%= link_to(edit_domain_path(domain), class: 'button tiny radius') do %>
              <i class="fa fa-times"></i>
              &nbsp;Edit Domain
            <% end %>
          </td>
        </tr>
      <% end %>
    <% end %>
  </tbody>
</table>
<%= will_paginate @domains, previous_label: "Previous&nbsp;", next_label: "&nbsp;Next" %>


<p>&nbsp;</p>
<%= render :partial => "partials/add_new_object", locals: {model_name: 'domain'} %>

</div> </div>
